{
  "name": "tour-guide-app",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "develop": "concurrently \"gatsby develop -H 0.0.0.0\" \"npm run gql:codegen -- --watch\"",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "start": "npm run develop",
    "lint": "eslint .",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc \"**/!(*.?(s)css.d).+(js|jsx|ts|tsx|json|yaml|yml|css|sass|scss|md|mdx|graphql)\"",
    "prettier:format": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "validate": "npm run lint && npm run prettier:check && npm run typecheck",
    "test": "jest --config ./jest/jest.config.js --passWithNoTests",
    "test:coverage": "npm t -- --coverage",
    "test:watch": "npm t -- --watch",
    "gql:codegen": "graphql-codegen --config graphql-codegen.yml"
  },
  "author": "Alexey Ryabov <lesha_12_01@mail.ru>",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "react"
